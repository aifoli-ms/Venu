{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "status": 308,
            "headers": {
                "Location": "/homepage/index.html"
            }
        },
        {
            "src": "/(.+\\..+)",
            "dest": "/src/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ]
}